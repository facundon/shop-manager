{
   "name": "shop-manager",
   "version": "0.1.0",
   "engines": {
      "node": "14.15.5"
   },
   "productName": "Shop Manager",
   "author": {
      "name": "Facundo Rodriguez",
      "email": "facundonrodriguez@gmail.com"
   },
   "homepage": "./",
   "description": "Parser for CSV files emited by Calipri",
   "main": "public/electron.js",
   "repository": {
      "url": "https://github.com/facundon/shop-manager"
   },
   "private": true,
   "dependencies": {
      "@craco/craco": "^6.1.2",
      "better-sqlite3": "^7.4.1",
      "craco-babel-loader": "^0.1.4",
      "react": "^17.0.2",
      "react-dom": "^17.0.2",
      "react-scripts": "^4.0.3",
      "reflect-metadata": "^0.1.13",
      "typeorm": "^0.2.34",
      "web-vitals": "^1.0.1"
   },
   "devDependencies": {
      "@testing-library/jest-dom": "^5.11.4",
      "@testing-library/react": "^11.1.0",
      "@testing-library/user-event": "^12.1.10",
      "@types/jest": "^26.0.15",
      "@types/node": "^12.20.15",
      "@types/react": "^17.0.0",
      "@types/react-dom": "^17.0.0",
      "electron": "^13.1.2",
      "electron-builder": "^22.11.7",
      "electron-rebuild": "^2.3.5",
      "prettier": "^2.3.1",
      "sass": "^1.32.13",
      "typescript": "^4.1.2"
   },
   "scripts": {
      "start:web": "craco start",
      "start:desktop": "npm run build:desktop && electron ./build/electron.js",
      "build:web": "craco build",
      "build:desktop": "tsc -p electron/tsconfig.json",
      "test": "craco test",
      "eject": "craco eject",
      "dist": "electron-builder",
      "rebuild": "node_modules/.bin/electron-rebuild -f -w better-sqlite3",
      "prerelease": "npm run build:web",
      "release": "electron-builder -c.extraMetadata.main=build/electron.js"
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   }
}
